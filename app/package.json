    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-node-externals": "^1.7.2"
