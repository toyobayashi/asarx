{
  "name": "asarx",
  "version": "0.0.1",
  "description": "Electron archive asar browser.",
  "main": "./out/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node -P ./script/tsconfig.json ./script/webpack.dev.ts",
    "start:dev": "cross-env NODE_ENV=development ts-node -P ./script/tsconfig.json ./script/start.ts",
    "prod": "cross-env NODE_ENV=production ts-node -P ./script/tsconfig.json ./script/webpack.prod.ts",
    "start:prod": "cross-env NODE_ENV=production ts-node -P ./script/tsconfig.json ./script/start.ts",
    "start": "cross-env NODE_ENV=none ts-node -P ./script/tsconfig.json ./script/start.ts",
    "pack:ia32": "cross-env NODE_ENV=production ts-node -P ./script/tsconfig.json ./script/pack.ts ia32",
    "pack:x64": "cross-env NODE_ENV=production ts-node -P ./script/tsconfig.json ./script/pack.ts x64"
  },
  "keywords": [
    "react",
    "electron",
    "typescript"
  ],
  "author": "toyobayashi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toyobayashi/asarx.git"
  },
  "devDependencies": {
    "@types/archiver": "^2.1.3",
    "@types/electron-packager": "^13.0.0",
    "@types/fs-extra": "^5.1.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.12.30",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.6",
    "@types/react-router-dom": "^4.3.2",
    "@types/webpack": "^4.4.27",
    "@types/webpack-dev-server": "^3.1.5",
    "@types/webpack-node-externals": "^1.6.3",
    "archiver": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "4.2.0",
    "electron-packager": "^13.1.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.0.2",
